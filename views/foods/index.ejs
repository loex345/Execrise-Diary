<%- include('../partials/header') %>
<table class="table">
    <thead>
        <tr>
            <th>User Reporting</th>
            <th>Food Name</th>
            <th>Food Date</th>
            <th>Food MealNumber </th>
            <th>Food Servings</th>
            <th>Food Satiety</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody class="table table-striped">
        <% foods.forEach(function(f) { %>
          <tr>
            <td><%= f.foruser %></td>
            <td><%= f.foodName %></td>
            <td><%= f.date %></td>
            <td><%= f.mealNumber %></td>
            <td><%= f.servings %></td>
            <td><%= f.satiety %></td>
            <td><a href="/foods/<%=f.id%>">DETAILS</a></td>
          </tr>
        <%}); %>
        <!-- place index information here -->
    </tbody>
</table>
<%- include('../partials/footer') %>